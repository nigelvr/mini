x=5;

func double(y) {
    factor = 2;
    return factor*y;
}

z=20;

func add10(u) {
    return u+10;
}

func recsum(a,b) {
    if (b == 0) {
        return a;
    }
    return recsum(a+1,b-1);
}

func fib(n) {
    if (n == 0 or n == 1) {
        return 1;
    }
    return fib(n-1) + fib(n-2);
}

func fact(n) {
    if (n == 0) {
        return 1;
    }
    return n * fact(n-1);
}

func main() {
    arg = 10;
    return add10(double(recsum(x,z))) + fib(arg) + fact(arg);
}