func main() {
    L = [3, 5, 6, 1, 2, 7];
    i = 0;
    j = 0;
    while (i < 6) {
        j = i+1;
        while (j < 6) {
            if (L[i] > L[j]) {
                tmp = L[i];
                L[i] = L[j];
                L[j] = tmp;
            }
            j = j+1;
        }
        i = i+1;
    }
    print(L);
}
